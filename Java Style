var numeroSecreto = parseInt(Math.random() * 101)

while(chute != numeroSecreto) {
    var chute = prompt('Digite um numero entre 0 e 100')
    //se o chute for igual ao numer secreto
    if (chute == numeroSecreto) {
      alert('Parabéns você acertou')
      } else if (chute > numeroSecreto) {
        alert('Você Errou... o número secreto é menor que ' + chute )
      } else if (chute < numeroSecreto) {
        alert('Você errou... o número secreto é maior ' + chute )
      }
}
